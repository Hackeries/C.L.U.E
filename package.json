{
  "name": "clue-event-management",
  "version": "2.0.0",
  "description": "Modern full-stack event management system for colleges",
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "python manage.py runserver",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "install:all": "pip install -r requirements.txt && cd frontend && npm install",
    "setup": "python -m venv venv && pip install -r requirements.txt && cd frontend && npm install",
    "migrate": "python manage.py migrate",
    "collectstatic": "python manage.py collectstatic --noinput",
    "test:backend": "python manage.py test",
    "test:frontend": "cd frontend && npm run test"
  },
  "keywords": [
    "event-management",
    "django",
    "react",
    "supabase",
    "college",
    "university"
  ],
  "author": "C.L.U.E Team",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.9"
  },
  "dependencies": {
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.11.0"
  }
}
