{
  "version": 2,
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "clue.settings"
  },
  "buildCommand": "bash build.sh"
}
